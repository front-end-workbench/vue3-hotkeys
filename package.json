{
  "name": "vue3-hotkeys",
  "description": "useHotkeys for Vue 3",
  "keywords": [
    "vue3",
    "hotkey",
    "useHotkey",
    "useHotkeys",
    "shortcut",
    "shortcuts"
  ],
  "author": "Talljack <1442770751@qq.com>",
  "version": "0.0.1",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json --declarationDir dist/types",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/front-end-workbench/vue3-hotkeys"
  },
  "homepage": "https://github.com/front-end-workbench/vue3-hotkeys#readme",
  "dependencies": {
    "hotkeys-js": "^3.8.7"
  },
  "devDependencies": {
    "@testing-library/vue": "^5.6.2",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "4",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^1.2.2",
    "eslint": "^7.18.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "vue": "^3.1.5",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.0.24"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
